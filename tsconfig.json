{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@navigation": ["navigation/index.tsx"],
      "@screens/*": ["screens/*"],
      "@types": ["types/index.ts"]
    }
  }
}
